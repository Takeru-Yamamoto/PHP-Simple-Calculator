{
    "name": "takeru-yamamoto/simple-calculator",
    "description": "Simple Calculator for PHP",
    "type": "library",
    "license": "MIT",
    "version": "1.0",
    "autoload": {
        "psr-4": {
            "SimpleCalculator\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimpleCalculator\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "Calculator": "SimpleCalculator\\Laravel\\Facade\\Calculator"
            }
        }
    },
    "authors": [
        {
            "name": "Takeru Yamamoto",
            "email": "yamamo9150@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "takeru-yamamoto/static-proxy": "^1.0"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "laravel/laravel": "^11.0"
    }
}